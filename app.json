{
  "expo": {
    "name": "Sacred Sikkim",
    "slug": "sacred-sikkim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sacredsikkim",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-maps",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e2d9ceb5-c90f-4bb7-89ce-47f2ea17cc07"
      }
    },
    "android": {
      "package": "com.ashhhh1535.sacredsikkim"
    }
  }
}
